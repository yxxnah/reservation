type Link {
  id: ID! @id
  createdAt: DateTime! @createdAt
  description: String!
  url: String!  
}

type User {
  id: ID! @id
  createdAt: DateTime! @createdAt
  name: String!
  team: String!
}

enum Occupancy {
  FOUR
  SIX
  EIGHT
}

type Room {
  id: ID! @id
  createdAt: DateTime! @createdAt
  name: String!
  size: Occupancy!
}

type Booked {
  id: ID! @id
  createdAt: DateTime! @createdAt
  userId: String!
  roomId: String!
  started: String!
  ended: String!
}